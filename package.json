{
    "name": "aws-s3-archiver",
    "version": "1.0.3",
    "description": "Archives files from an AWS S3 bucket with stream and stores the zip file in S3.",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "lint": "eslint src __tests__",
        "lint:fix": "eslint src --fix",
        "test": "vitest --config ./vitest.config.ts",
        "coverage": "vitest run --coverage",
        "bulid": "unbuild"
    },
    "keywords": [
        "aws",
        "s3",
        "lambda",
        "archiver",
        "typescript"
    ],
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.873.0",
        "@aws-sdk/lib-storage": "^3.850.0",
        "archiver": "^7.0.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "@smithy/util-stream": "^4.1.2",
        "@types/archiver": "^6.0.3",
        "@types/eslint__js": "^9.14.0",
        "@types/node": "^22.13.9",
        "@vitest/coverage-v8": "^3.0.8",
        "aws-sdk-client-mock": "^4.1.0",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.1.1",
        "prettier": "^3.5.3",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.26.0",
        "unbuild": "^3.5.0",
        "vitest": "^3.0.8"
    },
    "author": "Wolfsin",
    "license": "MIT",
    "homepage": "https://github.com/Wolfsin/aws-s3-archiver#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Wolfsin/aws-s3-archiver.git"
    },
    "bugs": {
        "url": "https://github.com/Wolfsin/aws-s3-archiver/issues"
    }
}
